@page
@model IndexModel
@{
	ViewData["Title"] = "Home page";
}

<div class="text-center">
	<h1 class="display-1">Vasketid</h1>
	<form method="post" asp-page-handler="LogIn">
		<div class="mb-3 mt-3">
			<h3 class="display-6">Username</h3>
			<input type="number" asp-for="Userid" class="form-label" />
		</div>
		<div class="mb-3 mt-3">
			<h3 class="display-6">Password</h3>
			<input type="password" asp-for="Pass" class="form-label" />
		</div>
		<input type="submit" value="Log-in" />
	</form>
</div>
