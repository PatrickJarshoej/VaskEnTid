@page
@model VaskEnTidWeb.Pages.DomicileModel
@{
    ViewData["Title"] = "Domecile n shit";
}
<h1>@ViewData["Title"]</h1>

@foreach (VaskEnTidLib.Model.Domicile domicile in Model.Domiciles)
{
    <h1>
        Domicile Id: @domicile.DomicileID
    </h1>

    <p>
         Roadname: @domicile.Roadname
    </p>
    <p>
        Region: @domicile.Region
    </p>
    <p>
        City: @domicile.City
    </p>
    <p>
        Country: @domicile.Country
    </p>
    <p>
        Door: @domicile.Door
    </p>
    <p>
        Floor: @domicile.Floor
    </p>
    <p>
        Postalcode: @domicile.Postalcode
    </p>
    <p>
        User Ids
        @foreach (var i in domicile.UserIDs)
        {
           @i             
        }
    </p>
    <p>
        PriceTally: @domicile.PriceTally
    </p>
}